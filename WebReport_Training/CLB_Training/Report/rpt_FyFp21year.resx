  <data name="SqlDataSource1.SelectCommand" xml:space="preserve">
    <value>select CONVERT(VARCHAR(50), TNT_SubFyfp.GUID_Ref) AS GUID_Ref
,  CONVERT(VARCHAR(50), TNT_SubFyfp.GUID_Fyfp) AS GUID_Fyfp 
,RefName,TNT_SubFyfp.Patern_N,TrainDay_TotalHour
