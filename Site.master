                function openMasterWindows(name, title, width, height) {
                    btnID = name
                    var resolvedUrl = "";

<%--                    if (name == 'TAM6_002' || name == 'ตั้งชื่อการลา') { resolvedUrl = '<%= ResolveUrl("~/Form/FormSetting/WebMLeave.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }
