                    else if (name == 'TAM6_004' || name == 'กำหนดหมายเลขเครื่องรูดบัตร') { resolvedUrl = '<%= ResolveUrl("~/Form/FormSetting/WebNumberMachine.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }
                    else if (name == 'TAM6_005_03' || name == 'เงินพิเศษคิดต่อวัน เพิ่มเติม') { resolvedUrl = '<%= ResolveUrl("~/Form/FormSetting/WebConfigSp.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }
             
                   
                    else if (name == 'TAM2_005' || name == 'เพิ่มเวลา พนักงานแต่ละคน') { resolvedUrl = '<%= ResolveUrl("~/Form/FormProcess/WebWorkOutside.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }
                    else if (name == 'TAM1_005_04' || name == 'การขอโอทีแต่ละบุคคล') { resolvedUrl = '<%= ResolveUrl("~/Form/WebOTbyUser.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }
                    else if (name == 'TAM1_005_05' || name == 'การขอโอทีแบบพิเศษ แต่ละบุคคล') { resolvedUrl = '<%= ResolveUrl("~/Form/WebOTbyUserSp.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }
                    else if (name == 'TAM1_005_10' || name == 'การขอโอทีตกเบิก') { resolvedUrl = '<%= ResolveUrl("~/Form/WebOTOld.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }


                    else if (name == 'TAM1_006_06' || name == 'ขอสับเปลี่ยนวันหยุด') { resolvedUrl = '<%= ResolveUrl("~/Form/WebChangeHoliday.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }
                    else if (name == 'TAM1_006_08' || name == 'ตั้งกะงานช่วงวันที่') { resolvedUrl = '<%= ResolveUrl("~/Form/WebShiftbyuser.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }


                    else if (name == 'TAM1_006_07' || name == 'นำตารางกะงานเข้าจาก Text File') { resolvedUrl = '<%= ResolveUrl("~/Form/FormImport/WebImportShift_Global.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }
                    else if (name == 'TAM1_005_06' || name == 'รับข้อมูลการขอโอทีเข้าระบบ') { resolvedUrl = '<%= ResolveUrl("~/Form/FormImport/WebImportExcelOT.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }
                    else if (name == 'TAM1_005_07' || name == 'รับโอทีต่องวดเข้าระบบ จาก Text File (โอทีตกเบิก)') { resolvedUrl = '<%= ResolveUrl("~/Form/FormImport/WebImportOTPeriod.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }
                    else if (name == 'TAM1_003_04' || name == 'รับข้อมูลใบลาเข้าระบบ') { resolvedUrl = '<%= ResolveUrl("~/Form/FormImport/WebImportExcelLeave.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }
                    else if (name == 'TAM1_003_02' || name == 'การโอนข้อมูลลการลาสะสม Text File เข้าระบบ') { resolvedUrl = '<%= ResolveUrl("~/Form/FormImport/WebImportExcelLeaveHistory.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }

                    else if (name == "TAM5_003" || name == 'ตัวช่วยตั้งค่า') { resolvedUrl = '<%= ResolveUrl("~/Form/FormTool/WebSetSpecial.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }
                    else if (name == "TAM5_004" || name == 'ตัวช่วยตั้งแม่แบบบวันหยุดประจำปี') { resolvedUrl = '<%= ResolveUrl("~/Form/FormTool/WebAddMemo.aspx?Case=2")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }



                    else if (name == 'TAM5_005_02' || name == 'อัตโนมัติ') { resolvedUrl = '<%= ResolveUrl("~/Form/FormTool/WebSummerAuto.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }
                    else if (name == 'TAM5_005_03' || name == 'การนำสิทธื์พักร้อนเข้าระบบ') { resolvedUrl = '<%= ResolveUrl("~/Form/FormImport/WebImportSummer.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }

                    else if (name == "TAM6_009" || name == 'กำหนดวันที่ห้ามประมวลผลย้อนหลัง') { resolvedUrl = '<%= ResolveUrl("~/Form/FormProcess/WebConfigProcessTime.aspx")%>'; var oWnd = radopen(resolvedUrl, "QsfWindow"); }--%>

                    oWnd.setSize(width, height);
                    oWnd.set_title(title);

                    oWnd.set_behaviors(Telerik.Web.UI.WindowBehaviors.Move + Telerik.Web.UI.WindowBehaviors.Close);
                    oWnd.center();
                    oWnd.set_modal(true);
                    //oWnd.add_close(OnWindowClose);
                }

